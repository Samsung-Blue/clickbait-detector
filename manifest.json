{
  "manifest_version": 2,

  "name": "Clickbait",
  "description": "This extension shows percentage of clickbait content",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://127.0.0.1:5000/"
  ]
}
